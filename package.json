{
    "name": "zshelldoc",
    "version": "0.1.0",
    "description": "Documentation generator for Bash & Zsh, with call-trees, comment extraction, etc. ",
    "scripts": [
        "dist/bin/zsd",
        "dist/bin/zsd-detect",
        "dist/bin/zsd-to-adoc",
        "dist/bin/zsd-transform"
    ],
    "files": [
        "dist/doc/zshelldoc/LICENSE",
        "dist/doc/zshelldoc/NEWS",
        "dist/doc/zshelldoc/README.md",
        "dist/zshelldoc/zsd.config"
    ],
    "install": "make && make install"
}
